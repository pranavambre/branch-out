{
  "require": {
    "clubstudioltd/craft-inline-svg": "0.1.2",
    "craftcms/cms": "^3.5.10",
    "craftcms/redactor": "2.8.4",
    "fatfish/navigation": "1.0.8",
    "swdevelopment/animate": "v1.0.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
